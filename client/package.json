{
  "name": "apod",
  "version": "0.0.1",
  "author": "Ralph Schaer",
  "homepage": "https://golb.hplar.ch/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/Apod.proto -o src/protos/apod.js && pbts --no-comments src/protos/apod.js -o src/protos/apod.d.ts",
    "dist": "shx rm -rf www && npm run build --prod && node ./cache-busting.js && workbox inject:manifest && npm run minify-html && npm run compress",
    "compress": "gzip-all \"www/*.*\" && gzip-all \"www/build/*.*\"",
    "minify-html": "html-minifier -o www/index.html www/index.html --collapse-whitespace --remove-comments",
    "open": "http-server www -o -a localhost -p 1234"
  },
  "config": {
    "ionic_copy": "./copy.config.js",
    "ionic_webpack": "./webpack.config.js"
  },
  "dependencies": {
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/compiler-cli": "5.1.3",
    "@angular/core": "5.1.3",
    "@angular/forms": "5.1.3",
    "@angular/http": "5.1.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "dexie": "^2.0.1",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "protobufjs": "6.8.4",
    "rxjs": "5.5.6",
    "workbox-sw": "2.1.2",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.7",
    "gzip-all": "1.0.0",
    "html-minifier": "^3.5.8",
    "http-server": "0.10.0",
    "rev-hash": "2.0.0",
    "shx": "0.2.2",
    "typescript": "2.6.2",
    "webpack-manifest-plugin": "1.3.2",
    "workbox-cli": "2.1.2"
  },
  "description": "An Ionic project"
}
